{
	"extends": "@essex/tsconfig-base",
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Node",
		"jsx": "react-jsx",
		"lib": ["dom", "esnext"],
		"outDir": "lib",
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true
	},
	"include": ["src/**/*"]
}
